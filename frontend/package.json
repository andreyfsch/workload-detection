{
  "name": "workload-detection-frontend",
  "version": "1.0.0",
  "description": "Interface for feature selection that will consume the backend API",
  "main": "src/js/workload-detection-front.js",
  "scripts": {
    "generate:sprite": "svg-icon-generate --folder=src/img/icons/svg --output=src/img/icons/icon-sheet.svg",
    "optimize:svg": "svgo -f src/img/icons/svg -o src/img/icons/svg",
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "test": "semistandard",
    "fix": "semistandard --fix"
  },
  "keywords": [
    "scss",
    "javascript"
  ],
  "author": "Andrey Schoier",
  "license": "Apache-2.0",
  "devDependencies": {
    "@parcel/transformer-sass": "latest",
    "parcel": "latest",
    "sass": "latest",
    "semistandard": "latest",
    "svg-icon-sprite": "^1.1.1",
    "svgo": "^2.8.0"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "croppr": "^2.3.1",
    "fractional": "^1.0.0",
    "regenerator-runtime": "^0.13.7"
  }
}
